    "axios": "^1.6.8",
    "axios-retry": "^3.9.1",
    "connection-string": "^4.3.6",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.0.0",
    "fastify": "^4.8.1",
    "fastify-metrics": "^10.2.0",
    "fastify-plugin": "^4.0.0",
    "fastify-xml-body-parser": "^2.2.0",
    "fs-extra": "^10.0.1",
