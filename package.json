    "@types/node": "^20.11.5",
    "@types/pg": "^8.6.4",
    "@types/stream-buffers": "^3.0.7",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
